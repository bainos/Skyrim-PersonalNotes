{
    "$schema": "https://raw.githubusercontent.com/Exit-9B/MCM-Helper/main/docs/config.schema.json",
    "modName": "Wheeler - UI",
    "displayName": "$WHEELERUI_MODNAME",
    "minMcmVersion": 13,
    "cursorFillMode": "topToBottom",
    "content": [
        {
            "id" : "iPosition:Main",
            "text": "$WHEELERUI_POS",
            "type": "enum",
            "valueOptions": {
                "options": ["$WHEELERUI_POS_0", "$WHEELERUI_POS_1", "$WHEELERUI_POS_2", "$WHEELERUI_POS_3", "$WHEELERUI_POS_4", "$WHEELERUI_POS_5", "$WHEELERUI_POS_6", "$WHEELERUI_POS_7"],
                "sourceType": "ModSettingInt"
            }
        },
        {
            "id" : "iBlockMargin:Main",
            "text": "$WHEELERUI_BLKM",
            "type": "slider",
            "valueOptions": {
                "min" : 0,
                "max" : 500,
                "step" : 1,
                "sourceType": "ModSettingInt"
            }
        },
        {
            "id" : "iInlineMargin:Main",
            "text": "$WHEELERUI_INL",
            "type": "slider",
            "valueOptions": {
                "min" : 0,
                "max" : 500,
                "step" : 1,
                "sourceType": "ModSettingInt"
            }
        },
        {
            "id" : "iScale:Main",
            "text": "$WHEELERUI_SCL",
            "type": "slider",
            "valueOptions": {
                "min" : 0,
                "max" : 200,
                "step" : 1,
                "sourceType": "ModSettingInt"
            }
        }
    ]
}